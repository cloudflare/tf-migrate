    {
      "mode": "managed",
      "type": "cloudflare_teams_rule",
      "name": "with_settings",
      "provider": "provider[\"registry.terraform.io/cloudflare/cloudflare\"]",
      "instances": [
        {
          "schema_version": 0,
          "attributes": {
            "id": "with-settings-id",
            "account_id": "f037e56e89293a057740de681ac9abbe",
            "name": "Block Policy with Settings",
            "description": "Policy with custom block page",
            "precedence": 200,
            "action": "block",
            "enabled": true,
            "filters": ["dns"],
            "traffic": "",
            "identity": "",
            "device_posture": "",
            "version": 2,
            "rule_settings": [
              {
                "block_page_enabled": true,
                "block_page_reason": "Access to this site is blocked by company policy",
                "override_ips": ["1.1.1.1", "1.0.0.1"],
                "ip_categories": true
              }
            ]
          }
        }
      ]
    },
    {
      "mode": "managed",
      "type": "cloudflare_teams_rule",
      "name": "with_nested_blocks",
      "provider": "provider[\"registry.terraform.io/cloudflare/cloudflare\"]",
      "instances": [
        {
          "schema_version": 0,
          "attributes": {
            "id": "nested-blocks-id",
            "account_id": "f037e56e89293a057740de681ac9abbe",
            "name": "L4 Override Policy",
            "description": "Policy with L4 override and notification",
            "precedence": 300,
            "action": "l4_override",
            "enabled": true,
            "filters": ["l4"],
            "traffic": "",
            "identity": "",
            "device_posture": "",
            "version": 1,
            "rule_settings": [
              {
                "l4override": [
                  {
                    "ip": "192.168.1.100",
                    "port": 8080
                  }
                ],
                "notification_settings": [
                  {
                    "enabled": true,
                    "message": "This connection has been redirected",
                    "support_url": "https://support.example.com"
                  }
                ]
              }
            ]
          }
        }
      ]
    },
    {
